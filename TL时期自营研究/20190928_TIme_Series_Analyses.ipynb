    "from statsmodels.tsa.stattools import adfuller"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 证明不是random walk:"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "sr = [1, 2, 5, 3, 5, 7, 3, 6, 4, 7]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "(-2.841992800294026,\n",
       " 0.052521232148196734,\n",
       " 0,\n",
       " 9,\n",
       " {'1%': -4.473135048010974,\n",
       "  '5%': -3.28988060356653,\n",
       "  '10%': -2.7723823456790124},\n",
       " 22.84637995238255)"
      ]
     },
     "execution_count": 6,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "#方法一：Adfuller Test(ADF Test) 第一个值比%对应的值大是不好的，不能拒绝null hypothesis，说明是random walk\n",
    "# sr: returns of a price series.\n",
    "adfuller(sr)"
   ]
  },
